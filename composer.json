{
  "name": "enm/external-layout-bundle",
  "description": "This Bundle loads html layouts from remote, convert the layouts to twig templates and store it for local usage",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Philipp Marien",
      "email": "marien@eosnewmedia.de"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Enm\\Bundle\\ExternalLayoutBundle\\": "src"
    }
  },
  "require": {
    "php": ">=5.5",
    "ext-xml": "*",
    "guzzlehttp/guzzle": "^6.0",
    "psr/http-message": "^1.0",
    "symfony/framework-bundle": "^2.7|^3.0|^4.0",
    "symfony/filesystem": "^2.7|^3.0|^4.0",
    "symfony/css-selector": "^2.7|^3.0|^4.0",
    "symfony/event-dispatcher": "^2.7|^3.0|^4.0"
  }
}
